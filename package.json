{
  "name": "sakura-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "copy": "node_modules/.bin/copy-files-from-to --config copy-files-from-to.json",
    "clean": "node_modules/.bin/rimraf .build",
    "build": "npm run clean && nest build && npm run copy",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build && cross-env NODE_ENV=development nest start",
    "start:dev": "npm run build && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "npm run build && nest start --debug --watch",
    "start:prod": "npm run build && cross-env NODE_ENV=production pm2 start .build/src/main.js",
    "typeorm:dev": "cross-env NODE_ENV=development ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "typeorm:prod": "cross-env NODE_ENV=production ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "aws-lambda": "^1.0.6",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^9.0.2",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "object-mapper": "^6.2.0",
    "randomstring": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/randomstring": "^1.1.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "copy-files-from-to": "^3.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "plugin": "^0.3.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^1.1.9",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
